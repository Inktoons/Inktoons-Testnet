{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Content-Security-Policy",
                    "value": "frame-ancestors 'self' https://*.minepi.com https://minepi.com https://*.pinet.com https://pinet.com https://*.pi;"
                },
                {
                    "key": "Permissions-Policy",
                    "value": "publickey-credentials-get=*, payment=*, accelerometer=(), camera=(), gyroscope=(), microphone=()"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "ALLOWALL"
                },
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ]
}